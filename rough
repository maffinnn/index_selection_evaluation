SELECT attname, null_frac, avg_width, n_distinct, most_common_vals, histogram_bounds, correlation
FROM pg_stats
WHERE schemaname = 'public'
ORDER BY attname;

SELECT relname as tablename, n_tup_ins - n_tup_del as rowcount FROM pg_stat_all_tables WHERE schemaname = 'public';



select distinct(tablename) from pg_stats where schemaname = 'public';